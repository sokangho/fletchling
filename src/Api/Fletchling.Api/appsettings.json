{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq"
        }
      }
    ]
  },
  "WebUrls": "https://web-dev.fletchling.com;http://localhost:6002",
  "Firebase": {
    "ProjectId": "fletchling-dev"
  },
  "JwtConfig": {
    "Issuer": "http://localhost:6001",
    "Audience": "http://localhost:6001",
    "TokenValidityInDays": 7
  }
}
